<h1>You are now on the homepage!</h1>

<ol class="post-list">
    {{#each posts as |post|}}
    <li>
        <article class="post">
            <div class="title">
                <a href="{{post_url}}" target="_blank">{{title}}</a>
                <span>({{post_url}})</span>
            </div>
            <div class="meta">
            </div>
        </article>
    </li>
    {{/each}}
</ol> 

